<?xml version="1.0"?>
<openerp>
<data>

    <record model="ir.ui.view" id="view_sisr_base_dependencia_gerencia_form">
        <field name="name">sisr.base.dependencia_gerencia.form</field>
        <field name="model">sisr.base.dependencia_gerencia</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="sisr.base.dependencia_gerencia">
            <group col="4" colspan="2">
                <field name="codigo" select="1"/>
                <field name="nombre" select="0"/>
                <field name="ubicacion" colspan="4" select="0"/>
                <field name="tipo" select="2"/>
                <field name="direccion_id" select="0"/>
                <field name="gerencia_linea_ids" attrs="{'invisible':[('tipo','!=','general')]}" colspan="4" select="0"/>
                <field name="division_ids" attrs="{'invisible':[('tipo','!=','regional')]}" colspan="4" select="0"/>
                <field name="cfs_ids" attrs="{'invisible':[('tipo','!=','regional')]}" colspan="4" select="0"/>

            </group>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="view_sisr_base_dependencia_gerencia_tree">
        <field name="name">sisr.base.dependencia_gerencia.tree</field>
        <field name="model">sisr.base.dependencia_gerencia</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="sisr.base.dependencia_gerencia">
                <field name="codigo"/>
                <field name="tipo"/>
                <field name="nombre"/>
                <field name="ubicacion"/>
                <field name="direccion_id"/>

            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_sisr_base_dependencia_gerencia">
        <field name="name">Gerencias</field>
        <field name="res_model">sisr.base.dependencia_gerencia</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>
    

        
